# BuildBear GitHub Action Environment Variables
# Copy this file to .env for local development

# Environment
NODE_ENV=test

# BuildBear Configuration
BUILDBEAR_BASE_URL=https://api.test.buildbear.io
BUILDBEAR_TOKEN=e114cbdc-8c85-43e2-8a7f-818c375c7e33

# API Configuration
API_TIMEOUT=30000
API_RETRY_ATTEMPTS=3

# Sandbox Configuration
SANDBOX_MAX_RETRIES=10
SANDBOX_RETRY_DELAY=5000
SANDBOX_TIMEOUT=300000

# Logging Configuration
LOG_LEVEL=debug
DEBUG=true

# File Processing
MAX_FILE_SIZE=104857600
COMPRESSION_LEVEL=6

# GitHub Actions (automatically set in GitHub environment)
GITHUB_WORKSPACE=/github/workspace
GITHUB_REPOSITORY=owner/repo
GITHUB_RUN_ID=123456789
GITHUB_RUN_NUMBER=1
GITHUB_SHA=abc123def456
GITHUB_REF=refs/heads/main
GITHUB_ACTOR=username
GITHUB_RUN_ATTEMPT=1